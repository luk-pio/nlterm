{
  "name": "nlterm",
  "version": "0.2.0",
  "description": "nlterm is a command-line tool that translates natural language descriptions into terminal commands.",
  "author": "Lukasz Piotrak",
  "license": "MIT",
  "homepage": "https://github.com/luk-pio/nlterm",
  "repository": {
    "type": "git",
    "url": "https://github.com/luk-pio/nlterm.git"
  },
  "keywords": [
    "node",
    "cli",
    "terminal",
    "ai",
    "openai"
  ],
  "bugs": {
    "url": "https://github.com/luk-pio/nlterm/issues"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "bin": {
    "nlterm": "dist/index.cjs"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "bundle": "npm run clean && rollup -c && chmod u+x dist/index.cjs",
    "watch": "rollup -c -w",
    "build": "npm run bundle && mkdir -p bin && pkg -t node18-linux-arm64,node18-linux-x64,node18-macos-arm64,node18-macos-x64 package.json -C -o bin/nlterm --no-bytecode",
    "clean": "rm -rf dist bin",
    "test": "jest"
  },
  "dependencies": {
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.15.11",
    "jest": "^29.5.0",
    "pkg": "^5.8.1",
    "rollup": "^3.20.2",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "node": "~18"
  }
}
